{
	"name": "@imperium/core",
	"version": "4.0.0-alpha.7",
	"description": "Imperium Core package",
	"repository": "darkadept/imperium",
	"license": "MIT",
	"author": "Mike Kornelson <darkadept@durbn.net>",
	"engines": {
		"node": ">=12"
	},
	"bugs": {
		"url": "https://github.com/darkadept/imperium/issues"
	},
	"publishConfig": {
		"access": "public"
	},
	"main": "server.js",
	"types": "./types.d.ts",
	"files": [
		"lib",
		"client.js",
		"server.js",
		"types.d.ts"
	],
	"scripts": {
		"build": "yarn build:client && yarn build:server",
		"build:client": "NODE_ENV=production webpack --config webpack/client.js && NODE_ENV=development webpack --config webpack/client.js",
		"build:server": "NODE_ENV=production webpack --config webpack/server.js && NODE_ENV=development webpack --config webpack/server.js",
		"lint": "eslint --cache --ext js,ts,tsx src",
		"lint:fix": "eslint --cache --fix --ext js,ts,tsx src",
		"ts": "tsc",
		"yalc:publish": "yarn build && yalc publish --push"
	},
	"devDependencies": {
		"webpack-cli": "^3.3.9"
	},
	"dependencies": {
		"@imperium/util": "^4.0.0-alpha.7",
		"chalk": "^2.4.2",
		"debug": "^4.1.1",
		"express": "^4.17.1",
		"query-string": "^6.8.3",
		"react-hot-loader": "^4.12.15"
	}
}
