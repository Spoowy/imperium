{
	"name": "@imperium/graphql",
	"version": "4.0.0-alpha.1",
	"description": "Imperium Graphql package",
	"repository": "darkadept/imperium",
	"license": "MIT",
	"author": "Mike Kornelson <darkadept@durbn.net>",
	"engines": {
		"node": ">=12"
	},
	"bugs": {
		"url": "https://github.com/darkadept/imperium/issues"
	},
	"publishConfig": {
		"access": "public"
	},
	"types": "./types.d.ts",
	"files": [
		"server.js",
		"client.js",
		"config.js",
		"server.js.map",
		"client.js.map",
		"config.js.map",
		"types.d.ts"
	],
	"scripts": {
		"build": "yarn build:server && yarn build:client",
		"build:client": "NODE_ENV=production webpack --config webpack/client.js",
		"build:server": "NODE_ENV=production webpack --config webpack/server.js",
		"lint": "eslint --cache --ext js,ts,tsx src",
		"lint:fix": "eslint --cache --fix --ext js,ts,tsx src",
		"ts": "tsc",
		"yalc:publish": "yarn build && yalc publish --push"
	},
	"dependencies": {
		"@imperium/core": "^4.0.0-alpha.1",
		"@apollo/react-components": "^3.1.0",
		"@apollo/react-hooks": "^3.1.0",
		"@thx/date": "^10.3.0",
		"@thx/money": "^10.1.1",
		"apollo-cache": "^1.3.2",
		"apollo-cache-inmemory": "^1.6.3",
		"apollo-client": "^2.6.4",
		"apollo-link": "^1.2.13",
		"apollo-link-error": "^1.1.12",
		"apollo-link-http": "^1.5.16",
		"apollo-link-ws": "^1.0.19",
		"apollo-server-express": "^2.9.1",
		"apollo-utilities": "^1.3.2",
		"cors": "^2.8.5",
		"debug": "^4.1.1",
		"express-jwt": "^5.3.1",
		"graphql": "^14.5.6",
		"graphql-custom-types": "^1.5.1",
		"graphql-scalar-objectid": "^0.1.1",
		"graphql-tag": "^2.10.1",
		"lodash": "^4.17.15",
		"subscriptions-transport-ws": "^0.9.16"
	}
}
