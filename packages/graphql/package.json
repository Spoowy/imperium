{
	"name": "@imperium/graphql",
	"version": "0.0.1",
	"description": "Graphql Imperium package",
	"repository": "darkadept/imperium",
	"license": "MIT",
	"engines": {
		"node": ">=8"
	},
	"bugs": {
		"url": "https://github.com/darkadept/imperium/issues"
	},
	"files": [
		"lib",
		"README.md"
	],
	"main": "lib/server.js",
	"scripts": {
		"prepublish": "npm run build",
		"build": "npm run clean:lib && npm run build:lib",
		"build:watch": "npm run clean:lib && npm run build:lib -- --verbose -w",
		"build:lib": "NODE_ENV=production babel src -s --out-dir lib",
		"clean:lib": "rimraf ./lib/*",
		"lint": "eslint --cache 'src/**/*.js'"
	},
	"dependencies": {
		"@thx/date": "^7.1.3",
		"@thx/money": "^7.1.7",
		"apollo-cache-inmemory": "^1.2.7",
		"apollo-client": "^2.3.8",
		"apollo-link": "^1.2.2",
		"apollo-link-http": "^1.5.4",
		"apollo-server": "^2.0.0-rc.8",
		"apollo-server-express": "^2.0.0-rc.8",
		"debug": "^3.1.0",
		"express-jwt": "^5.3.0",
		"graphql": "^0.13.2",
		"graphql-custom-types": "^1.4.0",
		"graphql-playground-middleware-express": "^1.7.2",
		"graphql-tools": "^3.0.5",
		"lodash": "^4.17.4"
	},
	"peerDependencies": {
		"express": "^4.x"
	}
}
